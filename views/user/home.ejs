<%-include('../user/layout/user-header-layout.ejs') %>


  <aside id="fh5co-hero" class="js-fullheight">
    <div class="flexslider js-fullheight">
      <ul class="slides">

        <% banner.forEach(function(x) { %>
          <li style="background-image: url('/images/car/<%= x.image %>');">
            <div class="overlay-gradient"></div>
            <div class="container">
              <div class="col-md-6 col-md-offset-3 col-md-pull-3 js-fullheight slider-text">
                <div class="slider-text-inner">
                  <div class="desc">
                    <!-- <span class="price">$800</span> -->
                    <h2>
                      <%= x.bannerName %>
                    </h2>
                    <p>
                      <%= x.discription %>
                    </p>
                    <p>
                      <a href="/allproductpage" class="btn btn-primary btn-outline btn-lg">Shop Now</a>
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </li>

          <% }) %>

      </ul>
    </div>
  </aside>

  <div id="fh5co-product" style="background-color: black;">
    <div class="container">
      <div class="row animate-box">
        <div class="col-md-8 col-md-offset-2 text-center fh5co-heading" style="color: white;">
          <h2 style="color: white;">Some of our latest Products.</h2>
        </div>
      </div>
      <div class="row">
        <% products.forEach(function(x){ %>
          <div class="col-md-4 text-center animate-box">
            <div class="product">
              <div class="product-grid" style="background-image: url('/images/car/<%= x.image[0] %>')">
                <div class="inner">
                  <p>
                    <a href="/singleProduct/<%= x._id %>" class="icon"><i class="icon-eye"></i></a>
                  </p>
                </div>
              </div>
              <div class="desc">
                <h3><a href="/singleProduct/<%= x._id %>">
                    <%= x.brand.brand %>
                      <%= x.productName %>
                  </a></h3>
                <span class="price"> RS: <%= x.price %></span>
              </div>
            </div>
          </div>
          <% }) %>
      </div> <br>
      <div class="col-md-8 col-md-offset-2 text-center fh5co-heading">
      <form action="/allproductpage" method="get">
        <button type="submit" class="btn btn-round px-5" style="width: 200px; height: 50px; background-color: whitesmoke; color: black;">View All</button>
      </form>
    </div></div>
  </div>

